{{ template "base" . }}

{{ define "content" }}
    <div class="container">
        <div class="row">
            <div class="class col">
                <h1>Choose a room</h1>

                {{$rooms := index .Data "rooms"}}
                
                <ul>

                    {{ range $rooms }}
                        <li>{{.RoomName}}</li>
                    {{ end }}
                </ul>
                
            </div>
        </div>
    </div>
{{ end }}